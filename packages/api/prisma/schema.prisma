  quoteToken         Decimal?      @db.Decimal(78, 0)
  borrowedBaseToken  Decimal?      @db.Decimal(78, 0)
  borrowedQuoteToken Decimal?      @db.Decimal(78, 0)
  collateral         Decimal       @db.Decimal(78, 0)
