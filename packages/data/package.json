{
  "name": "data",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run sentry:sourcemaps",
    "dev": "concurrently \"pnpm run dev:service\" \"pnpm run dev:worker\"",
    "dev:service": "tsx watch src/service.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "start": "concurrently \"pnpm run start:service\" \"pnpm run start:worker\"",
    "start:service": "tsx src/service.ts",
    "start:worker": "tsx src/worker.ts",
    "start:reindex-market": "tsx src/worker.ts reindexMarket $*",
    "start:reindex-missing": "tsx src/worker.ts reindexMissing $*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org foil --project data ./dist && sentry-cli sourcemaps upload --org foil --project data ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/cli": "^2.39.0",
    "@sentry/node": "^8.40.0",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "p-retry": "^6.2.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.2",
    "viem": "^2.21.49"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.15.0",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": "9.x"
  }
}